a = int(input())
for i in range(a):
    b = int(input())
    my_list = input().split()
    c = int(input())
    my_list2 = input().split()
    cnt = 0
    for m in my_list:
        for n in my_list2:
            if m == n:
                cnt+=1
                break
    if cnt == b:
        print("True")
    else:
        print("False")
        